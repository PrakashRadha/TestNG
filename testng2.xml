<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	     <listeners>
		<listener class-name="com.testng.ReportsDemo"></listener>
	</listeners>
	<!-- Define parallel test execution -->
	<test name="Test">
		<groups>
			<define name="Sanity">
				<include name="Smoke"></include>
				<include name="Regression"></include>
			</define>
			<run>
				<include name="Sanity"></include>
			</run>
		</groups>
		<classes>
			<class name="com.testng.GroupingDemo"></class>
		</classes>
	</test>
</suite> <!--
Suite -->